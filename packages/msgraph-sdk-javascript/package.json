{
  "name": "@microsoft/msgraph-sdk-javascript",
  "version": "1.0.0-preview.10",
  "description": "Microsoft Graph JavaScript client library",
  "keywords": [
    "microsoft",
    "graph",
    "msgraph"
  ],
  "author": "Vincent Biret <vibiret@microsoft.com>",
  "homepage": "https://github.com/microsoftgraph/msgraph-sdk-typescript#readme",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoftgraph/msgraph-sdk-typescript.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rimraf -g **/*.d.ts && rimraf -g **/*.d.ts.map && rimraf -g **/*.js.map && rimraf -g **/*.js"
  },
  "bugs": {
    "url": "https://github.com/microsoftgraph/msgraph-sdk-typescript/issues"
  },
  "dependencies": {
    "@microsoft/kiota-abstractions": "file:../../../kiota-typescript/packages/abstractions",
    "@microsoft/kiota-http-fetchlibrary": "file:../../../kiota-typescript/packages/http/fetch",
    "@microsoft/kiota-serialization-form": "file:../../../kiota-typescript/packages/serialization/form",
    "@microsoft/kiota-serialization-json": "file:../../../kiota-typescript/packages/serialization/json",
    "@microsoft/kiota-serialization-multipart": "file:../../../kiota-typescript/packages/serialization/multipart",
    "@microsoft/kiota-serialization-text": "file:../../../kiota-typescript/packages/serialization/text",
    "guid-typescript": "^1.0.9",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@azure/identity": "^4.0.0",
    "@microsoft/kiota-authentication-azure": "file:../../../kiota-typescript/packages/authentication/azure",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.2",
    "chai": "^5.0.0",
    "mocha": "^10.2.0",
    "typescript": "^5.3.3"
  }
}
